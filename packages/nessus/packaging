#!/bin/bash

set -e # exit immediately if a simple command exits with a non-zero status
set -u # report the usage of uninitialized variables

# Copy common utilities
cp ${BOSH_COMPILE_TARGET}/common/utils.sh ${BOSH_INSTALL_TARGET}

# Copy nessus dummy utils script
cp ${BOSH_COMPILE_TARGET}/nessus/nessus_utils.sh ${BOSH_INSTALL_TARGET}

